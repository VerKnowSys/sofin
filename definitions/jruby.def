APP_FULL_NAME="JRuby"
APP_NAME="jruby"
APP_VERSION="1.7.0"
APP_HTTP_PATH="${MAIN_SOURCE_REPOSITORY}${APP_NAME}-bin-${APP_VERSION}.tar.gz"
APP_CONFIGURE_SCRIPT="binary"
APP_AFTER_UNPACK_CALLBACK="${MV_BIN} ${CACHE_DIR}cache/${APP_NAME}-${APP_VERSION}/${APP_NAME}-${APP_VERSION} ${PREFIX}"
APP_AFTER_INSTALL_CALLBACK="${PRINTF_BIN} 'gem: --no-ri --no-rdoc' > ${HOME}/.gemrc ; ${PREFIX}/bin/gem install bundler rails ; ${FIND_BIN} ${PREFIX} -type f -name '*.dll' -o -name '*.exe' -o -name '*.bat' -exec rm '{}' \;"
APP_EXPORTS="ast gem jgem irb jirb jruby jrubyc ri rdoc rake rails bundle"
