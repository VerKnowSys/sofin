APP_FULL_NAME="MySQL 5"
APP_SHA="06e1d856cfb1f98844ef92af47d4f4f7036ef294"
APP_NAME="mysql"
APP_VERSION="5.6.13"
APP_HTTP_PATH="${MAIN_SOURCE_REPOSITORY}${APP_NAME}-${APP_VERSION}.tar.gz"
APP_REQUIREMENTS="zlib bzip2 expat libiconv cmake ncurses"
case "${SYSTEM_NAME}" in
    Darwin)
        export APP_REQUIREMENTS="zlib bzip2 expat cmake ncurses"
        ;;
esac

APP_CONFIGURE_SCRIPT="cmake"
APP_EXPORTS="mysql mysql_config mysqld mysqld_multi mysqld_safe mysqladmin mysqlshow mysqlimport mysqlbinlog mysqldump mysqlaccess mysqlcheck mysqlhotcopy mysql_upgrade mysql_install_db"
APP_CONFLICTS_WITH="Mysql51"
