APP_FULL_NAME="Python 3"
APP_SHA="4c2d562a0681ba27bc920500050e2f08de224311"
APP_NAME="Python"
APP_VERSION="3.2.3"
APP_HTTP_PATH="${MAIN_SOURCE_REPOSITORY}${APP_NAME}-${APP_VERSION}.tar.bz2"
APP_CONFIGURE_ARGS="--with-fpectl --without-pydebug --disable-ipv6 --without-doc-strings --disable-tsc"
APP_REQUIREMENTS="zlib bzip2 yaml libiconv"
APP_EXPORTS="python3 python3-config easy_install pip"
if [ "${SYSTEM_NAME}" = "Darwin" ]; then
  export APP_CONFIGURE_ARGS="${APP_CONFIGURE_ARGS} --enable-universalsdk --with-universal-archs=intel"
fi
