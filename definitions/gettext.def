APP_FULL_NAME="GNU Gettext Library"
APP_NAME="gettext"
APP_VERSION="0.18.1.1"
APP_HTTP_PATH="${MAIN_SOURCE_REPOSITORY}${APP_NAME}-${APP_VERSION}.tar.gz"
APP_REQUIREMENTS="expat libiconv"
if [ "$($UNAME_BIN)" = "Darwin" ]; then
export APP_CONFIGURE_SCRIPT="binary"
export APP_HTTP_PATH="${MAIN_SOURCE_REPOSITORY}${APP_NAME}-${APP_VERSION}.mountainlion.bottle.tar.gz"
export APP_AFTER_UNPACK_CALLBACK="${CP_BIN} -r ${CACHE_DIR}cache/${APP_NAME}-${APP_VERSION}/${APP_NAME}/${APP_VERSION}/* ${PREFIX}"
fi