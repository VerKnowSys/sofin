APP_FULL_NAME="A Fast, Easy and Free BitTorrent client"
APP_NAME="transmission"
APP_VERSION="2.61"
APP_HTTP_PATH="${MAIN_SOURCE_REPOSITORY}${APP_NAME}-${APP_VERSION}.tar.xz"
APP_REQUIREMENTS="bzip2 perl openssl pkgconf curl libevent"
APP_CONFIGURE_ARGS="--enable-daemon --enable-cli --without-gtk --disable-mac --disable-nls"
APP_EXPORTS="transmission-daemon transmission-remote transmission-cli transmission-create transmission-edit transmission-show"
