DISABLE_ON="Darwin Linux"
APP_FULL_NAME="OpenJDK 1.6 64bit"
APP_SHA="44878ef560c866fb45390dd76457ec38973e5435"
APP_NAME="Openjdk6-amd64"
APP_VERSION="1.6.0_32"
APP_HTTP_PATH="${MAIN_BINARY_REPOSITORY}${APP_NAME}_${APP_VERSION}.tar.gz"
APP_CONFIGURE_SCRIPT="binary"
APP_AFTER_INSTALL_CALLBACK="${CP_BIN} -fR ${CACHE_DIR}cache/${APP_NAME}-${APP_VERSION}/${APP_NAME}/ ${PREFIX}; ${RM_BIN} -f ${PREFIX}/lib/libiconv.so.3 ; ${RM_BIN} -f ${PREFIX}/lib/libcharset.so.1 ; ${RM_BIN} -f ${PREFIX}/lib/libintl.so.9"
APP_JAVA_ANCESTOR="JDK6_64"
APP_USER_INFO="OpenJDK notes:
  1. This is FreeBSD binary build.
  2. This definition also sets JAVA_HOME to /Software/YOUR-VERSION.
  3. Consider using own JAVA_HOME to override default value for user side Openjdk.
  4. Whole JAVA ENV is set through $HOME/.profile_java"