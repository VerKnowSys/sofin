DISABLE_ON="Darwin"
APP_FULL_NAME="Open Andrew File System"
APP_NAME="openafs"
APP_VERSION="1.6.1"
APP_HTTP_PATH="${MAIN_SOURCE_REPOSITORY}${APP_NAME}-${APP_VERSION}-src.tar.gz"
APP_CONFIGURE_ARGS="--disable-fuse-client --with-bsd-kernel-build=/usr/obj/usr/src/sys/GENERIC/"
KERNEL_MODULE_FILE="/boot/modules/libafs.ko"
APP_AFTER_INSTALL_CALLBACK="${CP_BIN} -vf ${PREFIX}/lib/openafs/libafs.ko ${KERNEL_MODULE_FILE}; ${KLDLOAD_BIN} ${KERNEL_MODULE_FILE}; ${MKDIR_BIN} -p ${PREFIX}/var/openafs/logs; ${MKDIR_BIN} -p ${PREFIX}/etc/openafs; echo 'done'"
APP_EXPORTS="afsd bos_util butc fms fstrace kadb_check kdb prdb_check read_tape rxdebug state_analyzer vldb_check voldump vos backup bosserver dafssync-debug fssync-debug ka-forwarder kas kpwvalid pt_util rmtsysd salvsync-debug uss vldb_convert volinfo vsys fs_compile_et afsmonitor asetkey cmdebug klog klog.krb5 kpasswd livesys pagsh.krb restorevol scout tokens translate_et unlog xstat_cm_test afsio aklog bos fs klog.krb knfs kpwvalid pagsh pts rxgen sys tokens.krb udebug up xstat_fs_test"