. ./config
APP_FULL_NAME="Apache Ant"
APP_NAME="apache-ant"
APP_VERSION="1.8.2"
APP_HTTP_PATH="${MAIN_SOURCE_REPOSITORY}${APP_NAME}-${APP_VERSION}-bin.tar.gz"
APP_CONFIGURE_SCRIPT="binary"
APP_REQUIREMENTS="openjdk"
APP_AFTER_INSTALL_CALLBACK="$CP_BIN -R ${TMP_ROOT}${APP_NAME}/${APP_NAME}-${APP_VERSION} ${SOFTWARE_DIR}Apache-ant-${APP_VERSION} && $FIND_BIN ${SOFTWARE_DIR}Apache-ant-${APP_VERSION} -name '*.bat' -delete && $FIND_BIN ${SOFTWARE_DIR}Apache-ant-${APP_VERSION} -name '*.cmd' -delete"