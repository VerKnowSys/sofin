DISABLE_ON="Darwin" # use prebuilt version from Trolltech/Nokia site
APP_FULL_NAME="Qt4 Core Library. This build doesn't include any GUI applications."
APP_NAME="qt"
APP_SHA="745f9ebf091696c0d5403ce691dc28c039d77b9e"
APP_VERSION="4.8.5"
APP_HTTP_PATH="${MAIN_SOURCE_REPOSITORY}${APP_NAME}-everywhere-opensource-src-${APP_VERSION}.tar.gz"
APP_CONFIGURE_ARGS="-prefix ${PREFIX} -release -opensource -confirm-license -shared -no-largefile -no-accessibility -stl -no-qt3support -no-xmlpatterns -no-multimedia -no-audio-backend -no-phonon -no-phonon-backend -webkit -script -javascript-jit -scripttools -no-declarative -qt-zlib -no-libtiff -no-libpng -no-libmng -no-libjpeg -openssl -optimized-qmake -no-cups -no-iconv -no-pch -no-dbus -no-nas-sound -no-gtkstyle -no-opengl -no-openvg -no-sm -no-xsync -no-xshape -no-xinerama -no-xcursor -no-xfixes -no-xrandr -no-xrender -no-mitshm -no-fontconfig -no-xinput -no-xkb -exceptions -no-gui -fast -verbose -silent -nomake examples -nomake demos -nomake docs -nomake translations -continue"
APP_EXPORTS="moc lconvert lrelease lupdate qdoc3 qmake rcc"
APP_REQUIREMENTS="gmake"
APP_MAKE_METHOD="gmake -s ${MAKE_OPTS}"
APP_INSTALL_METHOD="gmake install -s ${MAKE_OPTS}"
