DISABLE_ON="Linux"
APP_FULL_NAME="Google V8 JavaScript Virtual Machine"
APP_NAME="v8"
APP_SHA="595492842ff86eaa7e8cf5cf88b2dd9a000e357f"
APP_VERSION="3.14.5"
APP_HTTP_PATH="${MAIN_SOURCE_REPOSITORY}${APP_NAME}-${APP_VERSION}.tar.gz"
APP_CONFIGURE_SCRIPT="ignore"
APP_REQUIREMENTS="libiconv python-legacy scons" # libexecinfo is part of ServeD base system hence it's excluded from requirements
APP_MAKE_METHOD="scons ${MAKE_OPTS} arch=x64 mode=release snapshot=on library=static debuggersupport=off werror=no strictaliasing=off liveobjectlist=off I_know_I_should_build_with_GYP=yes"
APP_INSTALL_METHOD="${CP_BIN} libv8.a ${PREFIX}/lib/ && ${CP_BIN} libv8preparser.a ${PREFIX}/lib/ && ${CP_BIN} -r include/ ${PREFIX}/include"
APP_NO_CCACHE="YES"
FORCE_GNU_COMPILER="YES"