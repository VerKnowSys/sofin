APP_FULL_NAME="Google V8 JavaScript Virtual Machine"
APP_NAME="v8"
APP_VERSION="3.9.5"
APP_HTTP_PATH="${MAIN_SOURCE_REPOSITORY}${APP_NAME}-${APP_VERSION}.tar.gz"
APP_CONFIGURE_SCRIPT="ignore"
APP_REQUIREMENTS="libiconv python-legacy scons libexecinfo"
APP_MAKE_METHOD="true"
APP_COMPILER_ARGS="-L${PREFIX}/lib"
APP_INSTALL_METHOD="scons -j2 arch=x64 mode=release snapshot=off library=shared debuggersupport=off werror=no strictaliasing=off"
APP_AFTER_INSTALL_CALLBACK="${MV_BIN} lib*.so ${PREFIX}/lib/ && ${MV_BIN} include/* ${PREFIX}/include/"
