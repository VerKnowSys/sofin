APP_FULL_NAME="Apache Maven"
APP_NAME="apache-maven"
APP_VERSION="3.0.4"
APP_HTTP_PATH="${MAIN_SOURCE_REPOSITORY}${APP_NAME}-${APP_VERSION}-bin.tar.gz"
APP_CONFIGURE_SCRIPT="binary"
APP_AFTER_UNPACK_CALLBACK="${MV_BIN} ${CACHE_DIR}cache/${APP_NAME}-${APP_VERSION}/${APP_NAME}-${APP_VERSION} ${PREFIX}"
APP_AFTER_INSTALL_CALLBACK="${FIND_BIN} ${PREFIX} -type f -name '*.dll' -o -name '*.exe' -o -name '*.bat' -exec rm '{}' \;"
APP_EXPORTS="mvn mvnDebug mvnyjp"
