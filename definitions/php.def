. ./config
APP_FULL_NAME="PHP"
APP_NAME="php"
APP_VERSION="5.3.6"
APP_HTTP_PATH="${MAIN_SOURCE_REPOSITORY}${APP_NAME}-${APP_VERSION}.tar.gz"
APP_CONFIGURE_ARGS="--with-layout=GNU --disable-all --with-libxml-dir=$SOFTWARE_DIR/Php-$APP_VERSION --with-pcre-regex=$SOFTWARE_DIR/Php-$APP_VERSION --with-zlib-dir=$SOFTWARE_DIR/Php-$APP_VERSION --with-gettext=$SOFTWARE_DIR/Php-$APP_VERSION --with-gd=$SOFTWARE_DIR/Php-$APP_VERSION --with-png-dir=$SOFTWARE_DIR/Php-$APP_VERSION --with-jpeg-dir=$SOFTWARE_DIR/Php-$APP_VERSION --enable-exif --enable-zend-multibyte --without-apache --disable-debug --disable-ipv6 --enable-fpm"
APP_REQUIREMENTS="make iconv gettext zlib bzip2 m4 bison xml pcre exif expat png jpeg freetype fontconfig gd"

