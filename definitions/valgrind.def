APP_FULL_NAME="Valgrind for BSD"
APP_NAME="valgrind"
APP_VERSION="3.7.0-3"
APP_HTTP_PATH="${MAIN_SOURCE_REPOSITORY}${APP_NAME}-freebsd-${APP_VERSION}.tar.gz"
APP_CONFIGURE_ARGS="--enable-only64bit CFLAGS='-g -O0 -fPIC -DWITH_DEBUG'"
APP_REQUIREMENTS="pkgconf bzip2 make zlib perl expat libiconv gettext"
APP_EXPORTS="valgrind valgrind-listener no_op_client_for_valgrind callgrind_annotate callgrind_control cg_annotate cg_diff cg_merge"
