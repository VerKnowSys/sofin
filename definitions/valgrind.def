APP_FULL_NAME="Valgrind for BSD"
APP_NAME="valgrind"
APP_VERSION="3.6.1-2"
APP_HTTP_PATH="${MAIN_SOURCE_REPOSITORY}${APP_NAME}-freebsd-${APP_VERSION}.tar.gz"
APP_CONFIGURE_ARGS="--enable-tls --enable-only64bit"
