#!/usr/bin/env sh
# @author: Daniel (dmilith) Dettlaff (dmilith at me dot com)

. share/sofin/loader

if [ "Darwin" = "${SYSTEM_NAME}" ]; then
    s util Ccache Bash Make Pkgconf Perl Python27 Cmake Zip M4 Curl-minimal Ninja Wget Yasm Nasm Gawk Scons Automake Automake14 Automake18
else
    s util Ccache Doas M4 Bash Make Pkgconf Perl Python27 Cmake Zip M4 Curl-minimal Ninja Wget Yasm Nasm Gawk Scons Automake Automake14 Automake18
fi
