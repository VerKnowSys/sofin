#!/usr/bin/env sh
# @author: Daniel (dmilith) Dettlaff (dmilith at me dot com)

. share/sofin/loader

if [ "Darwin" = "${SYSTEM_NAME}" ]; then
    "${SOFIN_ROOT}/bin/s" util Ccache Bash Make Pkgconf Perl Python27 Cmake Zip M4 Ninja Wget Yasm Nasm Gawk Scons Automake Automake14 Automake18
else
    "${SOFIN_ROOT}/bin/s" util Ccache Doas M4 Bash Make Pkgconf Perl Python27 Cmake Zip M4 Ninja Wget Yasm Nasm Gawk Scons Automake Automake14 Automake18
fi
