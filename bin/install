#!/bin/sh
# @author: Daniel (dmilith) Dettlaff (dmilith at me dot com)

test -f src/sofin.conf.sh && . src/sofin.conf.sh
test -f /etc/profile_sofin && . /etc/profile_sofin

PREFIX="${PREFIX}" INSTALL="YES" bin/build
if [ "$PURE" != "" ]; then
    note "Pure mode"
    REPOSITORY= sofin setup
fi

bin/clean
